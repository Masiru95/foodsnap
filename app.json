{
  "expo": {
    "name": "Snapfood",
    "slug": "foodsnap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "foodsnap",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.masiru.snapfood",
      "infoPlist": {
        "NSCameraUsageDescription": "FoodSnap necesita acceso a la c�mara para tomar fotos de tus alimentos y hacer an�lisis nutricional.",
        "NSPhotoLibraryUsageDescription": "FoodSnap necesita acceso a la biblioteca de fotos para seleccionar im�genes de alimentos para su an�lisis."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FF6B6B"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.masiru.snapfood",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "com.android.vending.BILLING",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1a1a1a"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#FF6B6B"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "FoodSnap necesita acceso a la c�mara para tomar fotos de tus alimentos."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "FoodSnap necesita acceso a tus fotos para analizar im�genes de alimentos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "8c8768a4-10f3-4846-894a-25b99f25db6d"
      }
    },
    "owner": "masiru"
  }
}
